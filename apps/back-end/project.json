{
  "name": "back-end",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/back-end/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": ["--node-env=production"],
        "cwd": "apps/back-end"
      },
      "configurations": {
        "development": {
          "args": ["--node-env=development"]
        }
      }
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/back-end/package.json",
        "{workspaceRoot}/dist/apps/back-end/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": ["{workspaceRoot}/dist/apps/back-end/workspace_modules"],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "back-end:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "back-end:build:development"
        },
        "production": {
          "buildTarget": "back-end:build:production"
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    },
    "db-up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose up -d postgres",
        "cwd": "apps/back-end"
      }
    },
    "db-stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose stop postgres",
        "cwd": "apps/back-end"
      }
    },
    "db-nuke": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose down -v",
        "cwd": "apps/back-end"
      }
    },
    "docker-api-rebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose up -d --no-deps --build api",
        "cwd": "apps/back-end"
      }
    },
    "docker-api-stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose stop api",
        "cwd": "apps/back-end"
      }
    },
    "docker-up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose up -d --build",
        "cwd": "apps/back-end"
      }
    },
    "docker-down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose down",
        "cwd": "apps/back-end"
      }
    },
    "docker-logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose logs -f",
        "cwd": "apps/back-end"
      }
    },
    "typeorm": {
      "executor": "nx:run-commands",
      "options": {
        "command": "typeorm-ts-node-commonjs -d src/config/typeorm.config.ts",
        "cwd": "apps/back-end",
        "env": {
          "TS_NODE_PROJECT": "tsconfig.typeorm.json"
        }
      }
    },
    "migration-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "typeorm-ts-node-commonjs -d src/config/typeorm.config.ts migration:generate src/database/migrations/{args.name}",
        "cwd": "apps/back-end",
        "env": {
          "TS_NODE_PROJECT": "tsconfig.typeorm.json"
        }
      }
    },
    "migration-run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "typeorm-ts-node-commonjs -d src/config/typeorm.config.ts migration:run",
        "cwd": "apps/back-end",
        "env": {
          "TS_NODE_PROJECT": "tsconfig.typeorm.json"
        }
      }
    },
    "seed-run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node src/database/seeds/main.seed.ts",
        "cwd": "apps/back-end",
        "env": {
          "TS_NODE_PROJECT": "tsconfig.typeorm.json"
        }
      }
    }
  }
}
